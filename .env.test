# Test environment configuration
# This file is committed to git and used for running tests

APP_ENV=test
KERNEL_CLASS='App\Kernel'
APP_SECRET='test_secret_not_for_production'
SYMFONY_DEPRECATIONS_HELPER=999999

# Test database (use db_test container in Docker)
DATABASE_URL="postgresql://app:app_password@db_test:5432/app_test?serverVersion=16&charset=utf8"

# JWT configuration for tests
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=test_passphrase_not_for_production
